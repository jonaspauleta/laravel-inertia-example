{
    "private": true,
    "scripts": {
        "old_dev": "npm run development",
        "dev": "mix",
        "dev-ssr": "mix --mix-config=webpack.ssr.mix.js",
        "development": "mix",
        "watch": "mix watch",
        "watch-ssr": "mix watch --mix-config=webpack.ssr.mix.js",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "prod-ssr": "mix --production && mix --production --mix-config=webpack.ssr.mix.js",
        "production": "mix --production",
        "ssr": "mix && mix --mix-config=webpack.ssr.mix.js"
    },
    "devDependencies": {
        "@vue/compiler-sfc": "^3.2.30",
        "autoprefixer": "^10.4.2",
        "axios": "^0.25",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.4.8",
        "tailwindcss": "^3.0.23",
        "vue-loader": "^16.8.3"
    },
    "dependencies": {
        "@inertiajs/inertia": "^0.11.0",
        "@inertiajs/inertia-vue3": "^0.6.0",
        "@inertiajs/progress": "^0.2.7",
        "@inertiajs/server": "^0.1.0",
        "@vue/server-renderer": "^3.2.31",
        "postcss-import": "^14.0.2",
        "vue": "^3.2.29",
        "webpack-node-externals": "^3.0.0",
        "ziggy-js": "^1.4.4"
    }
}
